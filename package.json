{
  "name": "cv-job-matcher",
  "version": "1.0.0",
  "description": "Node.js server with tRPC for analyzing CV and job description alignment using AI",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "trpc",
    "pdf-analysis",
    "ai",
    "cv-matching",
    "node.js"
  ],
  "author": "Mudassir",
  "license": "MIT",
  "dependencies": {
    "@trpc/server": "^10.45.2",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "dotenv": "^17.0.0",
    "express": "^4.19.2",
    "multer": "^1.4.5-lts.1",
    "pdf-parse": "^1.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@trpc/client": "^10.45.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/pdf-parse": "^1.1.4",
    "@types/supertest": "^6.0.2",
    "form-data": "^4.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
